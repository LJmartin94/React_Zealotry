{
	"extends": "@react-native/typescript-config/tsconfig.json",
	"compilerOptions": {
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"jsx": "react-native",
		"baseUrl": "./",
		"lib": [
			"ESNext"
		],
		"paths": {
			"#features/*": [
				"./sources/features/*"
			],
			"#ui/*": [
				"./sources/ui/*"
			],
			"#assets/*": [
				"./assets/*"
			],
		}
	},
}